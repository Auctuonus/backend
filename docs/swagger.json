  "info": {
    "title": "Auctionus API",
    "description": "",
    "version": "1.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "securitySchemes": {
      "TMA": {
        "type": "apiKey",
        "in": "header",
        "name": "Authorization",
        "description": "Enter the Telegram Mini App init data in the format: tma <init-data>"
      },
      "JWT": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http",
        "description": "Enter your JWT access token (without Bearer prefix)"
      }
    },
    "schemas": {
      "LoginPasswordDto": {
        "type": "object",
        "properties": {
          "telegramId": {
            "type": "number",
            "description": "Telegram ID used as login",
            "example": 123456789
          },
          "password": {
            "type": "string",
            "description": "Password (telegram ID as string)",
            "example": "123456789",
            "minLength": 1
          }
        },
        "required": [
          "telegramId",
          "password"
        ]
      },
      "RefreshTokenDto": {
        "type": "object",
        "properties": {}
      },
      "GetAuctionListDto": {
        "type": "object",
        "properties": {}
      },
      "PlaceBidDto": {
        "type": "object",
        "properties": {}
      }
    }
  }
